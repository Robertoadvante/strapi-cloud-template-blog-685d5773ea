{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "cliente": {
      "type": "string"
    },
    "textreview": {
      "type": "blocks"
    },
    "stars": {
      "type": "integer",
      "required": true,
      "max": 5,
      "min": 1,
      "default": 5
    },
    "avatar": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
